		a.start();
		Thread.sleep(1000L);
		a.start();

		NumbersToWords ntw = new NumbersToWords();
		System.out.println(ntw.wordToNumbers("28931"));
		
		StringReverse.reverseString(new char[]{'a', 'b','c'},0,3);


		
		
		
	


